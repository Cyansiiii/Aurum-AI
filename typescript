export default {
  providers: [
    {
      domain: process.env.CONVEX_SITE_URL,
      applicationID: "convex",
    },
  ],
};

const handleGoogleLogin = async () => {
  setIsLoading(true);
  setError(null);
  try {
    await signIn("google");
  } catch (error) {
    console.error("Google login error:", error);
    setError("Failed to sign in with Google. Please try again.");
    setIsLoading(false);
  }
};